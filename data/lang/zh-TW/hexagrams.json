{
    "乾": {
        "guaText": "乾：元亨利貞。",
        "explanation": "大哉乾元，萬物資始，乃統天。雲行雨施，品物流形。大明終始，六位時成，時乘六龍以御天。乾道變化，各正性命，保合大和，乃利貞。首出庶物，萬國咸寧。",
        "yaoTexts": [
            {"position": "初九", "text": "潛龍勿用。"},
            {"position": "九二", "text": "見龍在田，利見大人。"},
            {"position": "九三", "text": "君子終日乾乾，夕惕若，厲，無咎。"},
            {"position": "九四", "text": "或躍在淵，無咎。"},
            {"position": "九五", "text": "飛龍在天，利見大人。"},
            {"position": "上九", "text": "亢龍有悔。"}
        ]
    },
    "坤": {
        "guaText": "坤：元亨利牝馬之貞。君子有攸往，先迷後得主，利西南得朋，東北喪朋。安貞吉。",
        "explanation": "至哉坤元，萬物資生，乃順承天。坤厚載物，德合無疆。含弘光大，品物咸亨。牝馬地類，行地無疆，柔順利貞。君子攸行，先迷失道，後順得常。西南得朋，乃與類行；東北喪朋，乃終有慶。安貞之吉，應地無疆。",
        "yaoTexts": [
            {"position": "初六", "text": "履霜，堅冰至。"},
            {"position": "六二", "text": "直，方，大，不習無不利。"},
            {"position": "六三", "text": "含章可貞。或從王事，無成有終。"},
            {"position": "六四", "text": "括囊，無咎無譽。"},
            {"position": "六五", "text": "黃裳，元吉。"},
            {"position": "上六", "text": "龍戰於野，其血玄黃。"}
        ]
    },
    "屯": {
        "guaText": "屯：元亨利貞，勿用有攸往，利建侯。",
        "explanation": "屯，剛柔始交而難生，動乎險中，大亨貞。雷雨之動滿盈，天造草昧，宜建侯而不寧。",
        "yaoTexts": [
            {"position": "初九", "text": "磐桓，利居貞，利建侯。"},
            {"position": "六二", "text": "屯如邅如，乘馬班如。匪寇婚媾，女子貞不字，十年乃字。"},
            {"position": "六三", "text": "即鹿無虞，惟入於林中，君子幾不如舍，往吝。"},
            {"position": "六四", "text": "乘馬班如，求婚媾，往吉無不利。"},
            {"position": "九五", "text": "屯其膏，小貞吉，大貞凶。"},
            {"position": "上六", "text": "乘馬班如，泣血漣如。"}
        ]
    },
    "蒙": {
        "guaText": "蒙：亨。匪我求童蒙，童蒙求我。初筮告，再三瀆，瀆則不告。利貞。",
        "explanation": "蒙，山下有險，險而止，蒙。蒙亨，以亨行時中也。匪我求童蒙，童蒙求我，志應也。初筮告，以剛中也。再三瀆，瀆則不告，瀆蒙也。蒙以養正，聖功也。",
        "yaoTexts": [
            {"position": "初六", "text": "發蒙，利用刑人，用說桎梏，以往吝。"},
            {"position": "九二", "text": "包蒙吉。納婦吉。子克家。"},
            {"position": "六三", "text": "勿用取女，見金夫，不有躬，無攸利。"},
            {"position": "六四", "text": "困蒙，吝。"},
            {"position": "六五", "text": "童蒙，吉。"},
            {"position": "上九", "text": "擊蒙，不利為寇，利禦寇。"}
        ]
    },
    "需": {
        "guaText": "需：有孚，光亨，貞吉。利涉大川。",
        "explanation": "需，須也。險在前也，剛健而不陷，其義不困窮矣。需有孚，光亨，貞吉，位乎天位，以正中也。利涉大川，往有功也。",
        "yaoTexts": [
            {"position": "初九", "text": "需于郊。利用恆，無咎。"},
            {"position": "九二", "text": "需于沙。小有言，終吉。"},
            {"position": "九三", "text": "需于泥，致寇至。"},
            {"position": "六四", "text": "需于血，出自穴。"},
            {"position": "九五", "text": "需于酒食，貞吉。"},
            {"position": "上六", "text": "入于穴，有不速之客三人來，敬之終吉。"}
        ]
    },
    "訟": {
        "guaText": "訟：有孚，窒惕，中吉，終凶。利見大人，不利涉大川。",
        "explanation": "訟，上剛下險，險而健，訟。訟有孚窒惕，中吉，剛來而得中也。終凶，訟不可成也。利見大人，尚中正也。不利涉大川，入于淵也。",
        "yaoTexts": [
            {"position": "初六", "text": "不永所事，小有言，終吉。"},
            {"position": "九二", "text": "不克訟，歸而逋，其邑人三百戶，無眚。"},
            {"position": "六三", "text": "食舊德，貞厲，終吉，或從王事，無成。"},
            {"position": "九四", "text": "不克訟，復即命，渝安貞，吉。"},
            {"position": "九五", "text": "訟元吉。"},
            {"position": "上九", "text": "或錫之鞶帶，終朝三褫之。"}
        ]
    },
    "師": {
        "guaText": "師：貞，丈人吉，無咎。",
        "explanation": "師，眾也，貞正也。能以眾正，可以王矣。剛中而應，行險而順，以此毒天下，而民從之，吉又何咎矣。",
        "yaoTexts": [
            {"position": "初六", "text": "師出以律，否臧凶。"},
            {"position": "九二", "text": "在師中吉，無咎，王三錫命。"},
            {"position": "六三", "text": "師或輿尸，凶。"},
            {"position": "六四", "text": "師左次，無咎。"},
            {"position": "六五", "text": "田有禽，利執言，無咎。長子帥師，弟子輿尸，貞凶。"},
            {"position": "上六", "text": "大君有命，開國承家，小人勿用。"}
        ]
    },
    "比": {
        "guaText": "比：吉。原筮，元永貞，無咎。不寧方來，後夫凶。",
        "explanation": "比，吉也，比，輔也，下順從也。原筮元永貞，無咎，以剛中也。不寧方來，上下應也。後夫凶，其道窮也。",
        "yaoTexts": [
            {"position": "初六", "text": "有孚比之，無咎。有孚盈缶，終來有它，吉。"},
            {"position": "六二", "text": "比之自內，貞吉。"},
            {"position": "六三", "text": "比之匪人。"},
            {"position": "六四", "text": "外比之，貞吉。"},
            {"position": "九五", "text": "顯比，王用三驅，失前禽。邑人不誡，吉。"},
            {"position": "上六", "text": "比之無首，凶。"}
        ]
    },
    "小畜": {
        "guaText": "小畜：亨。密雲不雨，自我西郊。",
        "explanation": "小畜，柔得位而上下應之，曰小畜。健而巽，剛中而志行，乃亨。密雲不雨，尚往也。自我西郊，施未行也。",
        "yaoTexts": [
            {"position": "初九", "text": "復自道，何其咎，吉。"},
            {"position": "九二", "text": "牽復，吉。"},
            {"position": "九三", "text": "輿說輻，夫妻反目。"},
            {"position": "六四", "text": "有孚，血去惕出，無咎。"},
            {"position": "九五", "text": "有孚攣如，富以其鄰。"},
            {"position": "上九", "text": "既雨既處，尚德載，婦貞厲。月幾望，君子征凶。"}
        ]
    },
    "履": {
        "guaText": "履：履虎尾，不咥人，亨。",
        "explanation": "履，柔履剛也。說而應乎乾，是以履虎尾，不咥人，亨。剛中正，履帝位而不疚，光明也。",
        "yaoTexts": [
            {"position": "初九", "text": "素履往，無咎。"},
            {"position": "九二", "text": "履道坦坦，幽人貞吉。"},
            {"position": "六三", "text": "眇能視，跛能履，履虎尾，咥人，凶。武人為於大君。"},
            {"position": "九四", "text": "履虎尾，愬愬終吉。"},
            {"position": "九五", "text": "夬履，貞厲。"},
            {"position": "上九", "text": "視履考祥，其旋元吉。"}
        ]
    },
    "泰": {
        "guaText": "泰：小往大來，吉亨。",
        "explanation": "泰，小往大來，吉亨。則是天地交而萬物通也，上下交而其志同也。內陽而外陰，內健而外順，內君子而外小人，君子道長，小人道消也。",
        "yaoTexts": [
            {"position": "初九", "text": "拔茅茹，以其彙，征吉。"},
            {"position": "九二", "text": "包荒，用馮河，不遐遺，朋亡，得尚於中行。"},
            {"position": "九三", "text": "無平不陂，無往不復，艱貞無咎。勿恤其孚，于食有福。"},
            {"position": "六四", "text": "翩翩，不富以其鄰，不戒以孚。"},
            {"position": "六五", "text": "帝乙歸妹，以祉元吉。"},
            {"position": "上六", "text": "城復于隍，勿用師，自邑告命，貞吝。"}
        ]
    },
    "否": {
        "guaText": "否：否之匪人，不利君子貞，大往小來。",
        "explanation": "否之匪人，不利君子貞，大往小來。則是天地不交而萬物不通也，上下不交而天下無邦也。內陰而外陽，內柔而外剛，內小人而外君子，小人道長，君子道消也。",
        "yaoTexts": [
            {"position": "初六", "text": "拔茅茹，以其彙，貞吉亨。"},
            {"position": "六二", "text": "包承。小人吉，大人否亨。"},
            {"position": "六三", "text": "包羞。"},
            {"position": "九四", "text": "有命無咎，疇離祉。"},
            {"position": "九五", "text": "休否，大人吉。其亡其亡，繫于苞桑。"},
            {"position": "上九", "text": "傾否，先否後喜。"}
        ]
    },
    "同人": {
        "guaText": "同人：同人于野，亨。利涉大川，利君子貞。",
        "explanation": "同人，所與上同也。同人曰，同人于野，亨。利涉大川，乾行也。文明以健，中正而應，君子正也。唯君子為能通天下之志。",
        "yaoTexts": [
            {"position": "初九", "text": "同人于門，無咎。"},
            {"position": "六二", "text": "同人于宗，吝。"},
            {"position": "九三", "text": "伏戎于莽，升其高陵，三歲不興。"},
            {"position": "九四", "text": "乘其墉，弗克攻，吉。"},
            {"position": "九五", "text": "同人，先號咷而後笑。大師克相遇。"},
            {"position": "上九", "text": "同人于郊，無悔。"}
        ]
    },
    "大有": {
        "guaText": "大有：元亨。",
        "explanation": "大有，柔得尊位大中，而上下應之，曰大有。其德剛健而文明，應乎天而時行，是以元亨。",
        "yaoTexts": [
            {"position": "初九", "text": "無交害，匪咎，艱則無咎。"},
            {"position": "九二", "text": "大車以載，有攸往，無咎。"},
            {"position": "九三", "text": "公用亨于天子，小人弗克。"},
            {"position": "九四", "text": "匪其彭，無咎。"},
            {"position": "六五", "text": "厥孚交如，威如，吉。"},
            {"position": "上九", "text": "自天祐之，吉無不利。"}
        ]
    },
    "謙": {
        "guaText": "謙：亨，君子有終。",
        "explanation": "謙，亨，天道下濟而光明，地道卑而上行。天道虧盈而益謙，地道變盈而流謙，鬼神害盈而福謙，人道惡盈而好謙。謙尊而光，卑而不可踰，君子之終也。",
        "yaoTexts": [
            {"position": "初六", "text": "謙謙君子，用涉大川，吉。"},
            {"position": "六二", "text": "鳴謙，貞吉。"},
            {"position": "九三", "text": "勞謙君子，有終吉。"},
            {"position": "六四", "text": "無不利，撝謙。"},
            {"position": "六五", "text": "不富以其鄰，利用侵伐，無不利。"},
            {"position": "上六", "text": "鳴謙，利用行師，征邑國。"}
        ]
    },
    "豫": {
        "guaText": "豫：利建侯行師。",
        "explanation": "豫，剛應而志行，順以動，豫。豫順以動，故天地如之，而況建侯行師乎？天地以順動，故日月不過，而四時不忒；聖人以順動，則刑罰清而民服。豫之時義大矣哉！",
        "yaoTexts": [
            {"position": "初六", "text": "鳴豫，凶。"},
            {"position": "六二", "text": "介于石，不終日，貞吉。"},
            {"position": "六三", "text": "盱豫，悔。遲有悔。"},
            {"position": "九四", "text": "由豫，大有得。勿疑，朋盍簪。"},
            {"position": "六五", "text": "貞疾，恆不死。"},
            {"position": "上六", "text": "冥豫，成有渝，無咎。"}
        ]
    },
    "隨": {
        "guaText": "隨：元亨利貞，無咎。",
        "explanation": "隨，剛來而下柔，動而說，隨。大亨貞，無咎，而天下隨時，隨時之義大矣哉！",
        "yaoTexts": [
            {"position": "初九", "text": "官有渝，貞吉。出門交有功。"},
            {"position": "六二", "text": "係小子，失丈夫。"},
            {"position": "六三", "text": "係丈夫，失小子。隨有求得，利居貞。"},
            {"position": "九四", "text": "隨有獲，貞凶。有孚在道，以明，何咎。"},
            {"position": "九五", "text": "孚于嘉，吉。"},
            {"position": "上六", "text": "拘係之，乃從維之。王用亨于西山。"}
        ]
    },
    "蠱": {
        "guaText": "蠱：元亨，利涉大川。先甲三日，後甲三日。",
        "explanation": "蠱，剛上而柔下，巽而止，蠱。蠱，元亨而天下治也。利涉大川，往有事也。先甲三日，後甲三日，終則有始，天行也。",
        "yaoTexts": [
            {"position": "初六", "text": "幹父之蠱，有子，考無咎，厲終吉。"},
            {"position": "九二", "text": "幹母之蠱，不可貞。"},
            {"position": "九三", "text": "幹父之蠱，小有悔，無大咎。"},
            {"position": "六四", "text": "裕父之蠱，往見吝。"},
            {"position": "六五", "text": "幹父之蠱，用譽。"},
            {"position": "上九", "text": "不事王侯，高尚其事。"}
        ]
    },
    "臨": {
        "guaText": "臨：元亨利貞。至于八月有凶。",
        "explanation": "臨，剛浸而長，說而順，剛中而應，大亨以正，天之道也。至于八月有凶，消不久也。",
        "yaoTexts": [
            {"position": "初九", "text": "咸臨，貞吉。"},
            {"position": "九二", "text": "咸臨，吉，無不利。"},
            {"position": "六三", "text": "甘臨，無攸利。既憂之，無咎。"},
            {"position": "六四", "text": "至臨，無咎。"},
            {"position": "六五", "text": "知臨，大君之宜，吉。"},
            {"position": "上六", "text": "敦臨，吉，無咎。"}
        ]
    },
    "觀": {
        "guaText": "觀：盥而不薦，有孚顒若。",
        "explanation": "大觀在上，順而巽，中正以觀天下。觀，盥而不薦，有孚顒若，下觀而化也。觀天之神道，而四時不忒，聖人以神道設教，而天下服矣。",
        "yaoTexts": [
            {"position": "初六", "text": "童觀，小人無咎，君子吝。"},
            {"position": "六二", "text": "窺觀，利女貞。"},
            {"position": "六三", "text": "觀我生，進退。"},
            {"position": "六四", "text": "觀國之光，利用賓于王。"},
            {"position": "九五", "text": "觀我生，君子無咎。"},
            {"position": "上九", "text": "觀其生，君子無咎。"}
        ]
    },
    "噬嗑": {
        "guaText": "噬嗑：亨。利用獄。",
        "explanation": "頤中有物曰噬嗑，噬嗑而亨。剛柔分，動而明，雷電合而章。柔得中而上行，雖不當位，利用獄也。",
        "yaoTexts": [
            {"position": "初九", "text": "屨校滅趾，無咎。"},
            {"position": "六二", "text": "噬膚滅鼻，無咎。"},
            {"position": "六三", "text": "噬腊肉，遇毒。小吝，無咎。"},
            {"position": "九四", "text": "噬乾胏，得金矢，利艱貞，吉。"},
            {"position": "六五", "text": "噬幹肉，得黃金，貞厲，無咎。"},
            {"position": "上九", "text": "何校滅耳，凶。"}
        ]
    },
    "賁": {
        "guaText": "賁：亨。小利有攸往。",
        "explanation": "賁，亨，柔來而文剛，故亨。分，剛上而文柔，故小利有攸往。天文也，文明以止，人文也。觀乎天文，以察時變；觀乎人文，以化成天下。",
        "yaoTexts": [
            {"position": "初九", "text": "賁其趾，舍車而徒。"},
            {"position": "六二", "text": "賁其須。"},
            {"position": "九三", "text": "賁如濡如，永貞吉。"},
            {"position": "六四", "text": "賁如皤如，白馬翰如，匪寇婚媾。"},
            {"position": "六五", "text": "賁于丘園，束帛戔戔，吝，終吉。"},
            {"position": "上九", "text": "白賁，無咎。"}
        ]
    },
    "剝": {
        "guaText": "剝：不利有攸往。",
        "explanation": "剝，剝也，柔變剛也。不利有攸往，小人長也。順而止之，觀象也。君子尚消息盈虛，天行也。",
        "yaoTexts": [
            {"position": "初六", "text": "剝床以足，蔑貞凶。"},
            {"position": "六二", "text": "剝床以辨，蔑貞凶。"},
            {"position": "六三", "text": "剝之，無咎。"},
            {"position": "六四", "text": "剝床以膚，凶。"},
            {"position": "六五", "text": "貫魚，以宮人寵，無不利。"},
            {"position": "上九", "text": "碩果不食，君子得輿，小人剝廬。"}
        ]
    },
    "復": {
        "guaText": "復：亨。出入無疾，朋來無咎。反復其道，七日來復，利有攸往。",
        "explanation": "復，亨，剛反，動而以順行，是以出入無疾，朋來無咎。反復其道，七日來復，天行也。利有攸往，剛長也。復，其見天地之心乎？",
        "yaoTexts": [
            {"position": "初九", "text": "不遠復，無祗悔，元吉。"},
            {"position": "六二", "text": "休復，吉。"},
            {"position": "六三", "text": "頻復，厲，無咎。"},
            {"position": "六四", "text": "中行獨復。"},
            {"position": "六五", "text": "敦復，無悔。"},
            {"position": "上六", "text": "迷復，凶，有災眚。用行師，終有大敗，以其國君凶，至于十年不克征。"}
        ]
    },
    "無妄": {
        "guaText": "無妄：元亨利貞。其匪正有眚，不利有攸往。",
        "explanation": "無妄，剛自外來而為主於內。動而健，剛中而應，大亨以正，天之命也。其匪正有眚，不利有攸往。無妄之往，何之矣？天命不佑，行矣哉？",
        "yaoTexts": [
            {"position": "初九", "text": "無妄往，吉。"},
            {"position": "六二", "text": "不耕獲，不愬畲，則利用攸往。"},
            {"position": "六三", "text": "無妄之災，或繫之牛，行人之得，邑人之災。"},
            {"position": "九四", "text": "可貞，無咎。"},
            {"position": "九五", "text": "無妄之疾，勿藥有喜。"},
            {"position": "上九", "text": "無妄行，有眚，無攸利。"}
        ]
    },
    "大畜": {
        "guaText": "大畜：利貞，不家食吉，利涉大川。",
        "explanation": "大畜，剛健篤實輝光，日新其德，剛上而尚賢。能止健，大正也。不家食吉，養賢也。利涉大川，應乎天也。",
        "yaoTexts": [
            {"position": "初九", "text": "有厲利已。"},
            {"position": "九二", "text": "輿說輹。"},
            {"position": "九三", "text": "良馬逐，利艱貞。曰閑輿衛，利有攸往。"},
            {"position": "六四", "text": "童牛之牿，元吉。"},
            {"position": "六五", "text": "豶豕之牙，吉。"},
            {"position": "上九", "text": "何天之衢，亨。"}
        ]
    },
    "頤": {
        "guaText": "頤：貞吉。觀頤，自求口實。",
        "explanation": "頤，貞吉，養正則吉也。觀頤，觀其所養也；自求口實，觀其自養也。天地養萬物，聖人養賢以及萬民。頤之時義大矣哉！",
        "yaoTexts": [
            {"position": "初九", "text": "舍爾靈龜，觀我朵頤，凶。"},
            {"position": "六二", "text": "顛頤，拂經，于丘頤，征凶。"},
            {"position": "六三", "text": "拂頤，貞凶，十年勿用，無攸利。"},
            {"position": "六四", "text": "顛頤吉，虎視眈眈，其欲逐逐，無咎。"},
            {"position": "六五", "text": "拂經，居貞吉，不可涉大川。"},
            {"position": "上九", "text": "由頤，厲吉，利涉大川。"}
        ]
    },
    "大過": {
        "guaText": "大過：棟橈，利有攸往，亨。",
        "explanation": "大過，大者過也。棟橈，本末弱也。剛過而中，巽而說行，利有攸往，乃亨。大過之時大矣哉！",
        "yaoTexts": [
            {"position": "初六", "text": "藉用白茅，無咎。"},
            {"position": "九二", "text": "枯楊生稊，老夫得其女妻，無不利。"},
            {"position": "九三", "text": "棟橈，凶。"},
            {"position": "九四", "text": "棟隆，吉；有它，吝。"},
            {"position": "九五", "text": "枯楊生華，老婦得士夫，無咎無譽。"},
            {"position": "上六", "text": "過涉滅頂，凶，無咎。"}
        ]
    },
    "坎": {
        "guaText": "坎：習坎，有孚，維心亨，行有尚。",
        "explanation": "習坎，重險也。水流而不盈，行險而不失其信。維心亨，乃以剛中也。行有尚，往有功也。天險不可升也，地險山川丘陵也，王公設險以守其國，坎之時用大矣哉！",
        "yaoTexts": [
            {"position": "初六", "text": "習坎，入于坎窞，凶。"},
            {"position": "九二", "text": "坎有險，求小得。"},
            {"position": "六三", "text": "來之坎坎，險且枕，入于坎窞，勿用。"},
            {"position": "六四", "text": "樽酒簋貳，用缶，納約自牖，終無咎。"},
            {"position": "九五", "text": "坎不盈，只既平，無咎。"},
            {"position": "上六", "text": "係用徽纆，寘于叢棘，三歲不得，凶。"}
        ]
    },
    "離": {
        "guaText": "離：利貞，亨。畜牝牛，吉。",
        "explanation": "離，麗也；日月麗乎天，百穀草木麗乎土，重明以麗乎正，乃化成天下。柔麗乎中正，故亨；是以畜牝牛吉也。",
        "yaoTexts": [
            {"position": "初九", "text": "履錯然，敬之，無咎。"},
            {"position": "六二", "text": "黃離，元吉。"},
            {"position": "九三", "text": "日昃之離，不鼓缶而歌，則大耋之嗟，凶。"},
            {"position": "九四", "text": "突如其來如，焚如，死如，棄如。"},
            {"position": "六五", "text": "出涕沱若，戚嗟若，吉。"},
            {"position": "上九", "text": "王用出征，有嘉折首，獲匪其醜，無咎。"}
        ]
    },
    "咸": {
        "guaText": "咸：亨。利貞。取女吉。",
        "explanation": "咸，感也。柔上而剛下，二氣感應以相與，止而說，男下女，是以亨利貞，取女吉也。天地感而萬物化生，聖人感人心而天下和平。觀其所感，而天地萬物之情可見矣。",
        "yaoTexts": [
            {"position": "初六", "text": "咸其拇。"},
            {"position": "六二", "text": "咸其腓，凶，居吉。"},
            {"position": "九三", "text": "咸其股，執其隨，往吝。"},
            {"position": "九四", "text": "貞吉悔亡，憧憧往來，朋從爾思。"},
            {"position": "九五", "text": "咸其脢，無悔。"},
            {"position": "上六", "text": "咸其輔頰舌。"}
        ]
    },
    "恆": {
        "guaText": "恆：亨。無咎，利貞，利有攸往。",
        "explanation": "恆，久也。剛上而柔下，雷風相與，巽而動，剛柔皆應，恆。恆，亨，無咎，利貞，久於其道也，天地之道，恆久而不已也。利有攸往，終則有始也。日月得天，而能久照，四時變化，而能久成，聖人久於其道，而天下化成。觀其所恆，而天地萬物之情可見矣。",
        "yaoTexts": [
            {"position": "初六", "text": "浚恆貞凶，無攸利。"},
            {"position": "九二", "text": "悔亡。"},
            {"position": "九三", "text": "不恆其德，或承之羞，貞吝。"},
            {"position": "九四", "text": "田無禽。"},
            {"position": "六五", "text": "恆其德貞，婦人吉，夫子凶。"},
            {"position": "上六", "text": "振恆凶。"}
        ]
    },
    "遯": {
        "guaText": "遯：亨，小利貞。",
        "explanation": "遯亨，遯而亨也。剛當位而應，與時行也。小利貞，浸而長也。遯之時義大矣哉！",
        "yaoTexts": [
            {"position": "初六", "text": "遯尾，厲，勿用有攸往。"},
            {"position": "六二", "text": "執之用黃牛之革，莫之勝說。"},
            {"position": "九三", "text": "系遯，有疾厲，畜臣妾吉。"},
            {"position": "九四", "text": "好遯，君子吉，小人否。"},
            {"position": "九五", "text": "嘉遯，貞吉。"},
            {"position": "上九", "text": "肥遯，無不利。"}
        ]
    },
    "損": {
        "guaText": "損：有孚，元吉，無咎，可貞，利有攸往。曷之用？二簋可用享。",
        "explanation": "損，損下益上，其道上行。損，節也。損而有孚，元吉，無咎，可貞，利有攸往。曷之用？二簋可用享。二簋，應有時。損剛益柔有時，損益盈虛，與時偕行。",
        "yaoTexts": [
            {"position": "初九", "text": "已事遄往，無咎，酌損之。"},
            {"position": "九二", "text": "利貞，征凶，弗損益之。"},
            {"position": "六三", "text": "三人行，則損一人；一人行，則得其友。"},
            {"position": "六四", "text": "損其疾，使遄有喜，無咎。"},
            {"position": "六五", "text": "或益之，十朋之龜弗克違，元吉。"},
            {"position": "上九", "text": "弗損益之，無咎，貞吉，利有攸往，得臣無家。"}
        ]
    },
    "大壯": {
        "guaText": "大壯：利貞。",
        "explanation": "大壯，大者壯也。剛以動，故壯。大壯利貞；大者正也。正大而天地之情可見矣！",
        "yaoTexts": [
            {"position": "初九", "text": "壯于趾，征凶，有孚。"},
            {"position": "九二", "text": "貞吉。"},
            {"position": "九三", "text": "小人用壯，君子用罔，貞厲。羝羊觸藩，羸其角。"},
            {"position": "九四", "text": "貞吉悔亡，藩決不羸，壯于大輿之輹。"},
            {"position": "六五", "text": "喪羊于易，無悔。"},
            {"position": "上六", "text": "羝羊觸藩，不能退，不能遂，無攸利，艱則吉。"}
        ]
    },
    "益": {
        "guaText": "益：利有攸往，利涉大川。",
        "explanation": "益，損上益下，民說無疆。自上下下，其道大光。利有攸往，中正有慶。利涉大川，木道乃行。益動而巽，日進無疆。天施地生，其益無方。凡益之道，與時偕行。",
        "yaoTexts": [
            {"position": "初九", "text": "利用為大作，元吉，無咎。"},
            {"position": "六二", "text": "或益之，十朋之龜弗克違，永貞吉。王用享于帝，吉。"},
            {"position": "六三", "text": "益之用凶事，無咎。有孚中行，告公用圭。"},
            {"position": "六四", "text": "中行，告公從。利用為依遷國。"},
            {"position": "九五", "text": "有孚惠心，勿問元吉。有孚惠我德。"},
            {"position": "上九", "text": "莫益之，或擊之，立心勿恆，凶。"}
        ]
    },
    "晉": {
        "guaText": "晉：康侯用錫馬蕃庶，晝日三接。",
        "explanation": "晉，進也。明出地上，順而麗乎大明，柔進而上行。是以康侯用錫馬蕃庶，晝日三接也。",
        "yaoTexts": [
            {"position": "初六", "text": "晉如，摧如，貞吉。罔孚，裕無咎。"},
            {"position": "六二", "text": "晉如，愁如，貞吉。受茲介福，于其王母。"},
            {"position": "六三", "text": "眾允，悔亡。"},
            {"position": "九四", "text": "晉如碩鼠，貞厲。"},
            {"position": "六五", "text": "悔亡，失得勿恤，往吉無不利。"},
            {"position": "上九", "text": "晉其角，維用伐邑，厲吉無咎，貞吝。"}
        ]
    },
    "明夷": {
        "guaText": "明夷：利艱貞。",
        "explanation": "明夷，明入地中，內文明而外柔順，以蒙大難，文王以之。利艱貞，晦其明也，內難而能正其志，箕子以之。",
        "yaoTexts": [
            {"position": "初九", "text": "明夷于飛，垂其翼。君子于行，三日不食。有攸往，主人有言。"},
            {"position": "六二", "text": "明夷，夷于左股，用拯馬壯，吉。"},
            {"position": "九三", "text": "明夷于南狩，得其大首，不可疾貞。"},
            {"position": "六四", "text": "入于左腹，獲明夷之心，出于門庭。"},
            {"position": "六五", "text": "箕子之明夷，利貞。"},
            {"position": "上六", "text": "不明晦，初登于天，后入于地。"}
        ]
    },
    "家人": {
        "guaText": "家人：利女貞。",
        "explanation": "家人，女正位乎內，男正位乎外，男女正，天地之大義也。家人有嚴君焉，父母之謂也。父父，子子，兄兄，弟弟，夫夫，婦婦，而家道正；正家而天下定矣。",
        "yaoTexts": [
            {"position": "初九", "text": "閑有家，悔亡。"},
            {"position": "六二", "text": "無攸遂，在中饋，貞吉。"},
            {"position": "九三", "text": "家人嗃嗃，悔厲吉；婦子嘻嘻，終吝。"},
            {"position": "六四", "text": "富家，大吉。"},
            {"position": "九五", "text": "王假有家，勿恤，吉。"},
            {"position": "上九", "text": "有孚威如，終吉。"}
        ]
    },
    "睽": {
        "guaText": "睽：小事吉。",
        "explanation": "睽，火動而上，澤動而下；二女同居，其志不同行；說而麗乎明，柔進而上行，得中而應乎剛；是以小事吉。天地睽，而其事同也；男女睽，而其志通也；萬物睽，而其事類也；睽之時用大矣哉！",
        "yaoTexts": [
            {"position": "初九", "text": "悔亡，喪馬勿逐，自復；見惡人，無咎。"},
            {"position": "九二", "text": "遇主于巷，無咎。"},
            {"position": "六三", "text": "見輿曳，其牛掣，其人天且劓，無初有終。"},
            {"position": "九四", "text": "睽孤，遇元夫，交孚，厲無咎。"},
            {"position": "六五", "text": "悔亡，厥宗噬膚，往何咎？"},
            {"position": "上九", "text": "睽孤，見豕負涂，載鬼一車，先張之弧，后說之弧，匪寇婚媾，往遇雨則吉。"}
        ]
    },
    "蹇": {
        "guaText": "蹇：利西南，不利東北；利見大人，貞吉。",
        "explanation": "蹇，難也，險在前也。見險而能止，知矣哉！蹇利西南，往得中也；不利東北，其道窮也。利見大人，往有功也。當位貞吉，以正邦也。蹇之時用大矣哉！",
        "yaoTexts": [
            {"position": "初六", "text": "往蹇，來譽。"},
            {"position": "六二", "text": "王臣蹇蹇，匪躬之故。"},
            {"position": "九三", "text": "往蹇來反。"},
            {"position": "六四", "text": "往蹇來連。"},
            {"position": "九五", "text": "大蹇，朋來。"},
            {"position": "上六", "text": "往蹇來碩，吉；利見大人。"}
        ]
    },
    "解": {
        "guaText": "解：利西南，無所往，其來復吉。有攸往，夙吉。",
        "explanation": "解，險以下，動而免乎險，解。解利西南，往得眾也。其來復吉，乃得中也。有攸往夙吉，往有功也。天地解，而雷雨作，雷雨作，而百果草木皆甲坼，解之時義大矣哉！",
        "yaoTexts": [
            {"position": "初六", "text": "無咎。"},
            {"position": "九二", "text": "田獲三狐，得黃矢，貞吉。"},
            {"position": "六三", "text": "負且乘，致寇至，貞吝。"},
            {"position": "九四", "text": "解而拇，朋至斯孚。"},
            {"position": "六五", "text": "君子維有解，吉；有孚于小人。"},
            {"position": "上六", "text": "公用射隼，于高墉之上，獲之，無不利。"}
        ]
    },
    "夬": {
        "guaText": "夬：揚于王庭，孚號，有厲，告自邑，不利即戎，利有攸往。",
        "explanation": "夬，決也，剛決柔也。健而說，決而和，揚于王庭，孚號有厲。其德剛健而文明也。告自邑，不利即戎，所尚乃穆也。利有攸往，剛長乃終也。",
        "yaoTexts": [
            {"position": "初九", "text": "壯于前趾，往不勝為咎。"},
            {"position": "九二", "text": "惕號，莫夜有戎，勿恤。"},
            {"position": "九三", "text": "壯于頄，有凶。君子夬夬，獨行遇雨，若濡有慍，無咎。"},
            {"position": "九四", "text": "臀無膚，其行次且。牽羊悔亡，聞言不信。"},
            {"position": "九五", "text": "莧陸夬夬，中行無咎。"},
            {"position": "上六", "text": "無號，終有凶。"}
        ]
    },
    "姤": {
        "guaText": "姤：女壯，勿用取女。",
        "explanation": "姤，遇也，柔遇剛也。勿用取女，不可與長也。天地相遇，品物咸章也。剛遇中正，天下大行也。姤之時義大矣哉！",
        "yaoTexts": [
            {"position": "初六", "text": "係于金柅，貞吉，有攸往，見凶，羸豕踟躅。"},
            {"position": "九二", "text": "包有魚，無咎，不利賓。"},
            {"position": "九三", "text": "臀無膚，其行次且，厲，無大咎。"},
            {"position": "九四", "text": "包無魚，起凶。"},
            {"position": "九五", "text": "以杞包瓜，含章，有隕自天。"},
            {"position": "上九", "text": "姤其角，吝，無咎。"}
        ]
    },
    "萃": {
        "guaText": "萃：亨。王假有廟，利見大人，亨，利貞。用大牲吉，利有攸往。",
        "explanation": "萃，聚也。順以說，剛中而應，故聚也。王假有廟，致孝享也。利見大人亨，聚以正也。用大牲吉，利有攸往，順天命也。觀其所聚，而天地萬物之情可見矣。",
        "yaoTexts": [
            {"position": "初六", "text": "有孚不終，乃亂乃萃，若號，一握為笑，勿恤，往無咎。"},
            {"position": "六二", "text": "引吉，無咎，孚乃利用禴。"},
            {"position": "六三", "text": "萃如，嗟如，無攸利，往無咎，小吝。"},
            {"position": "九四", "text": "大吉，無咎。"},
            {"position": "九五", "text": "萃有位，無咎。匪孚，元永貞，悔亡。"},
            {"position": "上六", "text": "齎咨涕洟，無咎。"}
        ]
    },
    "升": {
        "guaText": "升：元亨，用見大人，勿恤，南征吉。",
        "explanation": "升，柔以順，剛中而應，是以大亨。用見大人，勿恤，有慶也。南征吉，志行也。",
        "yaoTexts": [
            {"position": "初六", "text": "允升，大吉。"},
            {"position": "九二", "text": "孚乃利用禴，無咎。"},
            {"position": "九三", "text": "升虛邑。"},
            {"position": "六四", "text": "王用亨于岐山，吉，無咎。"},
            {"position": "六五", "text": "貞吉，升階。"},
            {"position": "上六", "text": "冥升，利于不息之貞。"}
        ]
    },
    "困": {
        "guaText": "困：亨，貞大人吉，無咎，有言不信。",
        "explanation": "困，剛掩也。險以說，困而不失其所亨，其唯君子乎？貞大人吉，以剛中也。有言不信，尚口乃窮也。",
        "yaoTexts": [
            {"position": "初六", "text": "臀困于株木，入于幽谷，三歲不覿。"},
            {"position": "九二", "text": "困于酒食，朱紱方來，利用享祀，征凶，無咎。"},
            {"position": "六三", "text": "困于石，據于蒺藜，入于其宮，不見其妻，凶。"},
            {"position": "九四", "text": "來徐徐，困于金車，吝，有終。"},
            {"position": "九五", "text": "劓刖，困于赤紱，乃徐有說，利用祭祀。"},
            {"position": "上六", "text": "困于葛藟，于臲卼，曰動悔，有悔，征吉。"}
        ]
    },
    "井": {
        "guaText": "井：改邑不改井，無喪無得，往來井井。汔至，亦未繘井，羸其瓶，凶。",
        "explanation": "巽乎水而上水，井。井養而不窮也。改邑不改井，乃以剛中也。汔至亦未繘井，未有功也。羸其瓶，是以凶也。",
        "yaoTexts": [
            {"position": "初六", "text": "井泥不食，舊井無禽。"},
            {"position": "九二", "text": "井谷射鮒，瓮敝漏。"},
            {"position": "九三", "text": "井渫不食，為我心惻，可用汲，王明，並受其福。"},
            {"position": "六四", "text": "井甃，無咎。"},
            {"position": "九五", "text": "井冽，寒泉食。"},
            {"position": "上六", "text": "井收勿幕，有孚元吉。"}
        ]
    },
    "革": {
        "guaText": "革：己日乃孚。元亨利貞，悔亡。",
        "explanation": "革，水火相息，二女同居，其志不相得，曰革。己日乃孚，革而信之。文明以說，大亨以正，革而當，其悔乃亡。天地革而四時成，湯武革命，順乎天而應乎人，革之時大矣哉！",
        "yaoTexts": [
            {"position": "初九", "text": "鞏用黃牛之革。"},
            {"position": "六二", "text": "己日乃革之，征吉，無咎。"},
            {"position": "九三", "text": "征凶，貞厲，革言三就，有孚。"},
            {"position": "九四", "text": "悔亡，有孚改命，吉。"},
            {"position": "九五", "text": "大人虎變，未占有孚。"},
            {"position": "上六", "text": "君子豹變，小人革面，征凶，居貞吉。"}
        ]
    },
    "鼎": {
        "guaText": "鼎：元吉，亨。",
        "explanation": "鼎，象也。以木巽火，亨飪也。聖人亨以享上帝，而大亨以養聖賢。巽而耳目聰明，柔進而上行，得中而應乎剛，是以元亨。",
        "yaoTexts": [
            {"position": "初六", "text": "鼎顛趾，利出否，得妾以其子，無咎。"},
            {"position": "九二", "text": "鼎有實，我仇有疾，不我能即，吉。"},
            {"position": "九三", "text": "鼎耳革，其行塞，雉膏不食，方雨虧悔，終吉。"},
            {"position": "九四", "text": "鼎折足，覆公餗，其形渥，凶。"},
            {"position": "六五", "text": "鼎黃耳金鉉，利貞。"},
            {"position": "上九", "text": "鼎玉鉉，大吉，無不利。"}
        ]
    },
    "震": {
        "guaText": "震：亨。震來虩虩，笑言啞啞。震驚百里，不喪匕鬯。",
        "explanation": "震，亨。震來虩虩，恐致福也。笑言啞啞，後有則也。震驚百里，驚遠而懼邇也。出可以守宗廟社稷，以為祭主也。",
        "yaoTexts": [
            {"position": "初九", "text": "震來虩虩，後笑言啞啞，吉。"},
            {"position": "六二", "text": "震來厲，億喪貝，躋于九陵，勿逐，七日得。"},
            {"position": "六三", "text": "震蘇蘇，震行無眚。"},
            {"position": "九四", "text": "震遂泥。"},
            {"position": "六五", "text": "震往來厲，億無喪，有事。"},
            {"position": "上六", "text": "震索索，視矍矍，征凶。震不于其躬，于其鄰，無咎。婚媾有言。"}
        ]
    },
    "艮": {
        "guaText": "艮：艮其背，不獲其身，行其庭，不見其人，無咎。",
        "explanation": "艮，止也。時止則止，時行則行，動靜不失其時，其道光明。艮其止，止其所也。上下敵應，不相與也。是以不獲其身，行其庭不見其人，無咎也。",
        "yaoTexts": [
            {"position": "初六", "text": "艮其趾，無咎，利永貞。"},
            {"position": "六二", "text": "艮其腓，不拯其隨，其心不快。"},
            {"position": "九三", "text": "艮其限，列其夤，厲薰心。"},
            {"position": "六四", "text": "艮其身，無咎。"},
            {"position": "六五", "text": "艮其輔，言有序，悔亡。"},
            {"position": "上九", "text": "敦艮，吉。"}
        ]
    },
    "漸": {
        "guaText": "漸：女歸吉，利貞。",
        "explanation": "漸之進也，女歸吉也。進得位，往有功也。進以正，可以正邦也。其位剛，得中也。止而巽，動不窮也。",
        "yaoTexts": [
            {"position": "初六", "text": "鴻漸于干，小子厲，有言，無咎。"},
            {"position": "六二", "text": "鴻漸于磐，飲食衎衎，吉。"},
            {"position": "九三", "text": "鴻漸于陸，夫征不復，婦孕不育，凶。利禦寇。"},
            {"position": "六四", "text": "鴻漸于木，或得其桷，無咎。"},
            {"position": "九五", "text": "鴻漸于陵，婦三歲不孕，終莫之勝，吉。"},
            {"position": "上九", "text": "鴻漸于逵，其羽可用為儀，吉。"}
        ]
    },
    "歸妹": {
        "guaText": "歸妹：征凶，無攸利。",
        "explanation": "歸妹，天地之大義也。天地不交，而萬物不興，歸妹，人之終始也。說以動，所歸妹也。征凶，位不當也。無攸利，柔乘剛也。",
        "yaoTexts": [
            {"position": "初九", "text": "歸妹以娣，跛能履，征吉。"},
            {"position": "九二", "text": "眇能視，利幽人之貞。"},
            {"position": "六三", "text": "歸妹以須，反歸以娣。"},
            {"position": "九四", "text": "歸妹愆期，遲歸有時。"},
            {"position": "六五", "text": "帝乙歸妹，其君之袂，不如其娣之袂良，月幾望，吉。"},
            {"position": "上六", "text": "女承筐無實，士刲羊無血，無攸利。"}
        ]
    },
    "豐": {
        "guaText": "豐：亨，王假之，勿憂，宜日中。",
        "explanation": "豐，大也。明以動，故豐。王假之，尚大也。勿憂宜日中，宜照天下也。日中則昃，月盈則食，天地盈虛，與時消息，而況於人乎？況於鬼神乎？",
        "yaoTexts": [
            {"position": "初九", "text": "遇其配主，雖旬無咎，往有尚。"},
            {"position": "六二", "text": "豐其蔀，日中見斗，往得疑疾，有孚發若，吉。"},
            {"position": "九三", "text": "豐其沛，日中見沫，折其右肱，無咎。"},
            {"position": "九四", "text": "豐其蔀，日中見斗，遇其夷主，吉。"},
            {"position": "六五", "text": "來章，有慶譽，吉。"},
            {"position": "上六", "text": "豐其屋，蔀其家，窺其戶，闃其無人，三歲不覿，凶。"}
        ]
    },
    "旅": {
        "guaText": "旅：小亨，旅貞吉。",
        "explanation": "旅，小亨，柔得中乎外，而順乎剛，止而麗乎明，是以小亨，旅貞吉也。旅之時義大矣哉！",
        "yaoTexts": [
            {"position": "初六", "text": "旅瑣瑣，斯其所取災。"},
            {"position": "六二", "text": "旅即次，懷其資，得童僕貞。"},
            {"position": "九三", "text": "旅焚其次，喪其童僕，貞厲。"},
            {"position": "九四", "text": "旅于處，得其資斧，我心不快。"},
            {"position": "六五", "text": "射雉一矢亡，終以譽命。"},
            {"position": "上九", "text": "鳥焚其巢，旅人先笑後號咷。喪牛于易，凶。"}
        ]
    },
    "巽": {
        "guaText": "巽：小亨，利有攸往，利見大人。",
        "explanation": "重巽以申命，剛巽乎中正而志行，柔皆順乎剛，是以小亨，利有攸往，利見大人。",
        "yaoTexts": [
            {"position": "初六", "text": "進退，利武人之貞。"},
            {"position": "九二", "text": "巽在床下，用史巫紛若，吉，無咎。"},
            {"position": "九三", "text": "頻巽，吝。"},
            {"position": "六四", "text": "悔亡，田獲三品。"},
            {"position": "九五", "text": "貞吉，悔亡，無不利。無初有終，先庚三日，後庚三日，吉。"},
            {"position": "上九", "text": "巽在床下，喪其資斧，貞凶。"}
        ]
    },
    "兌": {
        "guaText": "兌：亨，利貞。",
        "explanation": "兌，說也。剛中而柔外，說以利貞，是以順乎天，而應乎人。說以先民，民忘其勞；說以犯難，民忘其死；說之大，民勸矣哉！",
        "yaoTexts": [
            {"position": "初九", "text": "和兌，吉。"},
            {"position": "九二", "text": "孚兌，吉，悔亡。"},
            {"position": "六三", "text": "來兌，凶。"},
            {"position": "九四", "text": "商兌，未寧，介疾有喜。"},
            {"position": "九五", "text": "孚于剝，有厲。"},
            {"position": "上六", "text": "引兌。"}
        ]
    },
    "渙": {
        "guaText": "渙：亨。王假有廟，利涉大川，利貞。",
        "explanation": "渙，亨。剛來而不窮，柔得位乎外而上同，王假有廟，王乃在中也。利涉大川，乘木有功也。",
        "yaoTexts": [
            {"position": "初六", "text": "用拯馬壯，吉。"},
            {"position": "九二", "text": "渙奔其機，悔亡。"},
            {"position": "六三", "text": "渙其躬，無悔。"},
            {"position": "六四", "text": "渙其群，元吉。渙有丘，匪夷所思。"},
            {"position": "九五", "text": "渙汗其大號，渙王居，無咎。"},
            {"position": "上九", "text": "渙其血，去逖出，無咎。"}
        ]
    },
    "節": {
        "guaText": "節：亨。苦節不可貞。",
        "explanation": "節，亨，剛柔分，而剛得中。苦節不可貞，其道窮也。說以行剛，不伐堅也。利有攸往，剛柔皆應也。",
        "yaoTexts": [
            {"position": "初九", "text": "不出戶庭，無咎。"},
            {"position": "九二", "text": "不出門庭，凶。"},
            {"position": "六三", "text": "不節若，則嗟若，無咎。"},
            {"position": "六四", "text": "安節，亨。"},
            {"position": "九五", "text": "甘節，吉，往有尚。"},
            {"position": "上六", "text": "苦節，貞凶，悔亡。"}
        ]
    },
    "中孚": {
        "guaText": "中孚：豚魚吉，利涉大川，利貞。",
        "explanation": "中孚，柔在內而剛得中，說而巽，孚，乃化邦也。豚魚吉，信及豚魚也。利涉大川，乘木舟虛也。中孚以利貞，乃應乎天也。",
        "yaoTexts": [
            {"position": "初九", "text": "虞吉，有他不燕。"},
            {"position": "九二", "text": "鳴鶴在陰，其子和之，我有好爵，吾與爾靡之。"},
            {"position": "六三", "text": "得敵，或鼓或罷，或泣或歌。"},
            {"position": "六四", "text": "月幾望，馬匹亡，無咎。"},
            {"position": "九五", "text": "有孚攣如，無咎。"},
            {"position": "上九", "text": "翰音登于天，貞凶。"}
        ]
    },
    "小過": {
        "guaText": "小過：亨，利貞，可小事，不可大事。飛鳥遺之音，不宜上宜下，大吉。",
        "explanation": "小過，小者過而亨也。過以利貞，與時行也。柔得中，是以小事吉也。剛失位而不中，是以不可大事也。有飛鳥之象焉，有飛鳥遺之音，不宜上宜下，大吉；上逆而下順也。",
        "yaoTexts": [
            {"position": "初六", "text": "飛鳥以凶。"},
            {"position": "六二", "text": "過其祖，遇其妣；不及其君，遇其臣；無咎。"},
            {"position": "九三", "text": "弗過防之，從或戕之，凶。"},
            {"position": "九四", "text": "無咎，弗過遇之。往厲必戒，勿用永貞。"},
            {"position": "六五", "text": "密雲不雨，自我西郊，公弋取彼在穴。"},
            {"position": "上六", "text": "弗遇過之，飛鳥離之，凶，是謂災眚。"}
        ]
    },
    "既濟": {
        "guaText": "既濟：亨，小利貞，初吉終亂。",
        "explanation": "既濟，亨，小者亨也。利貞，剛柔正而位當也。初吉，柔得中也。終止則亂，其道窮也。",
        "yaoTexts": [
            {"position": "初九", "text": "曳其輪，濡其尾，無咎。"},
            {"position": "六二", "text": "婦喪其茀，勿逐，七日得。"},
            {"position": "九三", "text": "高宗伐鬼方，三年克之，小人勿用。"},
            {"position": "六四", "text": "繻有衣袽，終日戒。"},
            {"position": "九五", "text": "東鄰殺牛，不如西鄰之禴祭，實受其福。"},
            {"position": "上六", "text": "濡其首，厲。"}
        ]
    },
    "未濟": {
        "guaText": "未濟：亨，小狐汔濟，濡其尾，無攸利。",
        "explanation": "未濟，亨，柔得中也。小狐汔濟，未出中也。濡其尾，無攸利，不續終也。雖不當位，剛柔應也。",
        "yaoTexts": [
            {"position": "初六", "text": "濡其尾，吝。"},
            {"position": "九二", "text": "曳其輪，貞吉。"},
            {"position": "六三", "text": "未濟，征凶，利涉大川。"},
            {"position": "九四", "text": "貞吉，悔亡，震用伐鬼方，三年有賞于大國。"},
            {"position": "六五", "text": "貞吉，無悔，君子之光，有孚，吉。"},
            {"position": "上九", "text": "有孚于飲酒，無咎，濡其首，有孚失是。"}
        ]
    }
} 